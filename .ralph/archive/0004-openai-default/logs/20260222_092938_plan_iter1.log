rifyResult>;\n   135→\n   136→// --- Context Management ---\n   137→\n   138→export interface ContextManagementConfig {\n   139→  maxContextTokens?: number;\n   140→  keepRecentIterations?: number;\n   141→}\n   142→\n   143→// --- Middleware ---\n   144→\n   145→export interface MiddlewareContext {\n   146→  thread: AgentThread;\n   147→  iteration: number;\n   148→  settings: DeepFactorAgentSettings;\n   149→}\n   150→\n   151→export interface AgentMiddleware {\n   152→  name: string;\n   153→  tools?: StructuredToolInterface[];\n   154→  beforeIteration?: (ctx: MiddlewareContext) => Promise<void>;\n   155→  afterIteration?: (\n   156→    ctx: MiddlewareContext,\n   157→    result: unknown,\n   158→  ) => Promise<void>;\n   159→}\n   160→\n   161→// --- Agent Settings ---\n   162→\n   163→export interface DeepFactorAgentSettings<\n   164→  TTools extends StructuredToolInterface[] = StructuredToolInterface[],\n   165→> {\n   166→  model: BaseChatModel | string;\n   167→  tools?: TTools;\n   168→  instructions?: string;\n   169→  stopWhen?: StopCondition | StopCondition[];\n   170→  verifyCompletion?: VerifyCompletion;\n   171→  middleware?: AgentMiddleware[];\n   172→  interruptOn?: string[];\n   173→  contextManagement?: ContextManagementConfig;\n   174→  onIterationStart?: (iteration: number) => void;\n   175→  onIterationEnd?: (iteration: number, result: unknown) => void;\n   176→}\n   177→\n   178→// --- Results ---\n   179→\n   180→export interface AgentResult {\n   181→  response: string;\n   182→  thread: AgentThread;\n   183→  usage: TokenUsage;\n   184→  iterations: number;\n   185→  stopReason: \"completed\" | \"stop_condition\" | \"max_errors\";\n   186→  stopDetail?: string;\n   187→}\n   188→\n   189→export interface PendingResult {\n   190→  response: string;\n   191→  thread: AgentThread;\n   192→  usage: TokenUsage;\n   193→  iterations: number;\n   194→  stopReason: \"human_input_needed\";\n   195→  stopDetail?: string;\n   196→  resume: (humanResponse: string) => Promise<AgentResult | PendingResult>;\n   197→}\n   198→\n   199→export function isPendingResult(\n   200→  r: AgentResult | PendingResult,\n   201→): r is PendingResult {\n   202→  return r.stopReason === \"human_input_needed\";\n   203→}\n   204→"}]},"parent_tool_use_id":"toolu_018216ZSWQZoqzCG2pw1k3qr","session_id":"6ecbbd8c-6064-491f-935a-e3d372e67119","uuid":"5b37da7e-c6f7-4ea8-8175-66f64095a6e8"}
