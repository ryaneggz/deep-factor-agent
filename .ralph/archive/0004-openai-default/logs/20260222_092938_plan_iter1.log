   invoke: vi.fn().mockResolvedValue(\n   104→          new AIMessage({ content: \"Summary of iteration\" }),\n   105→        ),\n   106→      };\n   107→\n   108→      const events: MessageEvent[] = [];\n   109→      for (let i = 0; i < 5; i++) {\n   110→        events.push({\n   111→          type: \"message\",\n   112→          role: \"user\",\n   113→          content: `Iteration ${i} content`,\n   114→          timestamp: Date.now(),\n   115→          iteration: i,\n   116→        });\n   117→        events.push({\n   118→          type: \"message\",\n   119→          role: \"assistant\",\n   120→          content: `Response for iteration ${i}`,\n   121→          timestamp: Date.now(),\n   122→          iteration: i,\n   123→        });\n   124→      }\n   125→\n   126→      const thread = makeThread(events);\n   127→      const cm = new ContextManager({ keepRecentIterations: 3 });\n   128→\n   129→      const result = await cm.summarize(thread, mockModel as any);\n   130→\n   131→      const summaryEvents = result.events.filter(\n   132→        (e) => e.type === \"summary\",\n   133→      ) as SummaryEvent[];\n   134→      expect(summaryEvents.length).toBeGreaterThan(0);\n   135→\n   136→      const recentEvents = result.events.filter(\n   137→        (e) => e.type !== \"summary\" && e.iteration > 1,\n   138→      );\n   139→      expect(recentEvents.length).toBe(6);\n   140→    });\n   141→\n   142→    it(\"preserves recent iterations unchanged\", async () => {\n   143→      const events: MessageEvent[] = [\n   144→        {\n   145→          type: \"message\",\n   146→          role: \"user\",\n   147→          content: \"Hello\",\n   148→          timestamp: Date.now(),\n   149→          iteration: 1,\n   150→        },\n   151→        {\n   152→          type: \"message\",\n   153→          role: \"assistant\",\n   154→          content: \"Hi\",\n   155→          timestamp: Date.now(),\n   156→          iteration: 2,\n   157→        },\n   158→      ];\n   159→\n   160→      const thread = makeThread(events);\n   161→      const cm = new ContextManager({ keepRecentIterations: 3 });\n   162→\n   163→      const result = await cm.summarize(thread, {} as any);\n   164→      expect(result.events).toHaveLength(2);\n   165→    });\n   166→  });\n   167→\n   168→  describe(\"buildContextInjection\", () => {\n   169→    it(\"produces formatted string of iteration summaries\", () => {\n   170→      const summaryEvent: SummaryEvent = {\n   171→        type: \"summary\",\n   172→        summarizedIterations: [1, 2],\n   173→        summary: \"These iterations handled user authentication setup.\",\n   174→        timestamp: Date.now(),\n   175→        iteration: 1,\n   176→      };\n   177→\n   178→      const thread = makeThread([summaryEvent]);\n   179→      const cm = new ContextManager();\n   180→      const injection = cm.buildContextInjection(thread);\n   181→\n   182→      expect(injection).toContain(\"Previous Iteration Summaries\");\n   183→      expect(injection).toContain(\"1, 2\");\n   184→      expect(injection).toContain(\n   185→        \"These iterations handled user authentication setup.\",\n   186→      );\n   187→    });\n   188→\n   189→    it(\"returns empty string when no summaries exist\", () => {\n   190→      const thread = makeThread([\n   191→        {\n   192→          type: \"message\",\n   193→          role: \"user\",\n   194→          content: \"Hello\",\n   195→          timestamp: Date.now(),\n   196→          iteration: 1,\n   197→        },\n   198→      ]);\n   199→      const cm = new ContextManager();\n   200→      expect(cm.buildContextInjection(thread)).toBe(\"\");\n   201→    });\n   202→\n   203→    it(\"includes multiple summaries\", () => {\n   204→      const events: SummaryEvent[] = [\n   205→        {\n   206→          type: \"summary\",\n   207→          summarizedIterations: [1],\n   208→          summary: \"First iteration summary.\",\n   209→          timestamp: Date.now(),\n   210→          iteration: 1,\n   211→        },\n   212→        {\n   213→          type: \"summary\",\n   214→          summarizedIterations: [2, 3],\n   215→          summary: \"Second and third iteration summary.\",\n   216→          timestamp: Date.now(),\n   217→          iteration: 2,\n   218→        },\n   219→      ];\n   220→\n   221→      const thread = makeThread(events);\n   222→      const cm = new ContextManager();\n   223→      const injection = cm.buildContextInjection(thread);\n   224→\n   225→      expect(injection).toContain(\"Iteration(s) 1\");\n   226→      expect(injection).toContain(\"Iteration(s) 2, 3\");\n   227→      expect(injection).toContain(\"First iteration summary.\");\n   228→      expect(injection).toContain(\"Second and third iteration summary.\");\n   229→    });\n   230→  });\n   231→});\n   232→"}]},"parent_tool_use_id":"toolu_01CxRp8CNqnSsjp7xZS9EZ7N","session_id":"6ecbbd8c-6064-491f-935a-e3d372e67119","uuid":"81c60c2e-5c01-4d52-9990-b4b04fed7b83"}
