{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_0119NtgApNWiuGtoAQYYqRJr","type":"tool_result","content":"     1→import { tool } from \"@langchain/core/tools\";\n     2→import type { StructuredToolInterface } from \"@langchain/core/tools\";\n     3→import { z } from \"zod\";\n     4→import type {\n     5→  AgentMiddleware,\n     6→  MiddlewareContext,\n     7→  ErrorEvent,\n     8→} from \"./types.js\";\n     9→\n    10→export interface ComposedMiddleware {\n    11→  tools: StructuredToolInterface[];\n    12→  beforeIteration: (ctx: MiddlewareContext) => Promise<void>;\n    13→  afterIteration: (ctx: MiddlewareContext, result: unknown) => Promise<void>;\n    14→}\n    15→\n    16→export function composeMiddleware(\n    17→  middlewares: AgentMiddleware[],\n    18→): ComposedMiddleware {\n    19→  const tools: StructuredToolInterface[] = [];\n    20→\n    21→  for (const mw of middlewares) {\n    22→    if (mw.tools) {\n    23→      for (const t of mw.tools) {\n    24→        const existing = tools.findIndex((x) => x.name === t.name);\n    25→        if (existing >= 0) {\n    26→          console.warn(\n    27→            `Middleware tool conflict: \"${t.name}\" from \"${mw.name}\" overrides a previous definition`,\n    28→          );\n    29→          tools[existing] = t;\n    30→        } else {\n    31→          tools.push(t);\n    32→        }\n    33→      }\n    34→    }\n    35→  }\n    36→\n    37→  const beforeIteration = async (ctx: MiddlewareContext): Promise<void> => {\n    38→    for (const mw of middlewares) {\n    39→      if (mw.beforeIteration) {\n    40→        await mw.beforeIteration(ctx);\n    41→      }\n    42→    }\n    43→  };\n    44→\n    45→  const afterIteration = async (\n    46→    ctx: MiddlewareContext,\n    47→    result: unknown,\n    48→  ): Promise<void> => {\n    49→    for (const mw of middlewares) {\n    50→      if (mw.afterIteration) {\n    51→        await mw.afterIteration(ctx, result);\n    52→      }\n    53→    }\n    54→  };\n    55→\n    56→  return { tools, beforeIteration, afterIteration };\n    57→}\n    58→\n    59→export function todoMiddleware(): AgentMiddleware {\n    60→  const todoSchema = z.object({\n    61→    todos: z.array(\n    62→      z.object({\n    63→        id: z.string(),\n    64→        text: z.string(),\n    65→        status: z.enum([\"pending\", \"in_progress\", \"done\"]),\n    66→      }),\n    67→    ),\n    68→  });\n    69→\n    70→  return {\n    71→    name: \"todo\",\n    72→    tools: [\n    73→      tool(\n    74→        async (args: z.infer<typeof todoSchema>) => {\n    75→          return JSON.stringify({ success: true, todos: args.todos });\n    76→        },\n    77→        {\n    78→          name: \"write_todos\",\n    79→          description:\n    80→            \"Create or update the todo list for planning and tracking progress\",\n    81→          schema: todoSchema,\n    82→        },\n    83→      ),\n    84→      tool(\n    85→        async () => {\n    86→          return JSON.stringify({ todos: [] });\n    87→        },\n    88→        {\n    89→          name: \"read_todos\",\n    90→          description: \"Read the current todo list\",\n    91→          schema: z.object({}),\n    92→        },\n    93→      ),\n    94→    ],\n    95→  };\n    96→}\n    97→\n    98→export function errorRecoveryMiddleware(): AgentMiddleware {\n    99→  return {\n   100→    name: \"errorRecovery\",\n   101→    afterIteration: async (\n   102→      ctx: MiddlewareContext,\n   103→      _result: unknown,\n   104→    ): Promise<void> => {\n   105→      const { thread } = ctx;\n   106→      if (thread.events.length === 0) return;\n   107→\n   108→      const lastEvent = thread.events[thread.events.length - 1];\n   109→      if (lastEvent.type === \"error\") {\n   110→        const errorEvent = lastEvent as ErrorEvent;\n   111→        let errorMsg = errorEvent.error;\n   112→        if (errorMsg.length > 500) {\n   113→          errorMsg = errorMsg.substring(0, 500) + \"... [truncated]\";\n   114→        }\n   115→\n   116→        thread.events.push({\n   117→          type: \"message\",\n   118→          role: \"system\",\n   119→          content: `Error occurred: ${errorMsg}\\nConsider an alternative approach if the same error occurs again.`,\n   120→          timestamp: Date.now(),\n   121→          iteration: ctx.iteration,\n   122→        });\n   123→      }\n   124→    },\n   125→  };\n   126→}\n   127→"}]},"parent_tool_use_id":"toolu_01HisVqnKuiyZhTExZE4QzWw","session_id":"6ecbbd8c-6064-491f-935a-e3d372e67119","uuid":"9b5f65b6-c270-4b01-ad65-8afb12fcc719"}
