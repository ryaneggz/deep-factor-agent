{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01TzCSfPHWyeSGfqHF7irf2F","type":"tool_result","content":"     1→import type { BaseChatModel } from \"@langchain/core/language_models/chat_models\";\n     2→import { HumanMessage } from \"@langchain/core/messages\";\n     3→import type {\n     4→  AgentThread,\n     5→  ContextManagementConfig,\n     6→  SummaryEvent,\n     7→} from \"./types.js\";\n     8→\n     9→export function estimateTokens(text: string): number {\n    10→  if (text.length === 0) return 0;\n    11→  return Math.ceil(text.length / 3.5);\n    12→}\n    13→\n    14→export class ContextManager {\n    15→  private maxContextTokens: number;\n    16→  private keepRecentIterations: number;\n    17→\n    18→  constructor(config: ContextManagementConfig = {}) {\n    19→    this.maxContextTokens = config.maxContextTokens ?? 150000;\n    20→    this.keepRecentIterations = config.keepRecentIterations ?? 3;\n    21→  }\n    22→\n    23→  estimateThreadTokens(thread: AgentThread): number {\n    24→    let total = 0;\n    25→    for (const event of thread.events) {\n    26→      const serialized = JSON.stringify(event);\n    27→      total += estimateTokens(serialized);\n    28→    }\n    29→    return total;\n    30→  }\n    31→\n    32→  needsSummarization(thread: AgentThread): boolean {\n    33→    return this.estimateThreadTokens(thread) > this.maxContextTokens;\n    34→  }\n    35→\n    36→  async summarize(\n    37→    thread: AgentThread,\n    38→    model: BaseChatModel,\n    39→  ): Promise<AgentThread> {\n    40→    const iterationMap = new Map<number, typeof thread.events>();\n    41→    for (const event of thread.events) {\n    42→      const iter = event.iteration;\n    43→      if (!iterationMap.has(iter)) {\n    44→        iterationMap.set(iter, []);\n    45→      }\n    46→      iterationMap.get(iter)!.push(event);\n    47→    }\n    48→\n    49→    const iterations = Array.from(iterationMap.keys()).sort((a, b) => a - b);\n    50→    if (iterations.length === 0) return thread;\n    51→\n    52→    const maxIteration = iterations[iterations.length - 1];\n    53→    const cutoff = maxIteration - this.keepRecentIterations;\n    54→\n    55→    const oldIterations = iterations.filter((i) => i <= cutoff);\n    56→    if (oldIterations.length === 0) return thread;\n    57→\n    58→    const summaryEvents: SummaryEvent[] = [];\n    59→\n    60→    for (const iter of oldIterations) {\n    61→      const events = iterationMap.get(iter)!;\n    62→      if (events.length === 1 && events[0].type === \"summary\") continue;\n    63→\n    64→      const eventsText = events\n    65→        .map((e) => JSON.stringify(e))\n    66→        .join(\"\\n\");\n    67→\n    68→      try {\n    69→        const response = await model.invoke([\n    70→          new HumanMessage(\n    71→            `Summarize the following agent iteration events in 2-3 sentences. Focus on what tools were called, what was accomplished, and any errors:\\n\\n${eventsText}`,\n    72→          ),\n    73→        ]);\n    74→\n    75→        const content =\n    76→          typeof response.content === \"string\"\n    77→            ? response.content\n    78→            : JSON.stringify(response.content);\n    79→\n    80→        summaryEvents.push({\n    81→          type: \"summary\",\n    82→          summarizedIterations: [iter],\n    83→          summary: content,\n    84→          timestamp: Date.now(),\n    85→          iteration: iter,\n    86→        });\n    87→      } catch {\n    88→        summaryEvents.push({\n    89→          type: \"summary\",\n    90→          summarizedIterations: [iter],\n    91→          summary: `Iteration ${iter}: ${events.length} events (summarization failed)`,\n    92→          timestamp: Date.now(),\n    93→          iteration: iter,\n    94→        });\n    95→      }\n    96→    }\n    97→\n    98→    const newEvents = thread.events.filter(\n    99→      (e) => e.iteration > cutoff || e.type === \"summary\",\n   100→    );\n   101→\n   102→    thread.events = [...summaryEvents, ...newEvents];\n   103→    thread.updatedAt = Date.now();\n   104→\n   105→    return thread;\n   106→  }\n   107→\n   108→  buildContextInjection(thread: AgentThread): string {\n   109→    const summaries = thread.events.filter(\n   110→      (e): e is SummaryEvent => e.type === \"summary\",\n   111→    );\n   112→\n   113→    if (summaries.length === 0) return \"\";\n   114→\n   115→    let injection = \"## Previous Iteration Summaries\\n\\n\";\n   116→    for (const s of summaries) {\n   117→      const iters = s.summarizedIterations.join(\", \");\n   118→      injection += `### Iteration(s) ${iters}\\n${s.summary}\\n\\n`;\n   119→    }\n   120→\n   121→    return injection;\n   122→  }\n   123→}\n   124→"}]},"parent_tool_use_id":"toolu_018216ZSWQZoqzCG2pw1k3qr","session_id":"6ecbbd8c-6064-491f-935a-e3d372e67119","uuid":"f51a5c70-59a1-4b07-9662-f21ba919ba74"}
