{"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01Ec78ft9mmjKuRrMeadKMyC","type":"tool_result","content":"     1→import type { BaseChatModel } from \"@langchain/core/language_models/chat_models\";\n     2→import {\n     3→  HumanMessage,\n     4→  AIMessage,\n     5→  SystemMessage,\n     6→  ToolMessage,\n     7→} from \"@langchain/core/messages\";\n     8→import type { BaseMessage } from \"@langchain/core/messages\";\n     9→import type { StructuredToolInterface } from \"@langchain/core/tools\";\n    10→import { initChatModel } from \"langchain/chat_models/universal\";\n    11→import { composeMiddleware } from \"./middleware.js\";\n    12→import type { ComposedMiddleware } from \"./middleware.js\";\n    13→import { evaluateStopConditions } from \"./stop-conditions.js\";\n    14→import { ContextManager } from \"./context-manager.js\";\n    15→import { findToolByName } from \"./tool-adapter.js\";\n    16→import type {\n    17→  AgentThread,\n    18→  AgentResult,\n    19→  PendingResult,\n    20→  TokenUsage,\n    21→  StopCondition,\n    22→  DeepFactorAgentSettings,\n    23→  ToolCallEvent,\n    24→  ToolResultEvent,\n    25→  MessageEvent as AgentMessageEvent,\n    26→  ErrorEvent,\n    27→  CompletionEvent,\n    28→  HumanInputRequestedEvent,\n    29→  AgentMiddleware,\n    30→  MiddlewareContext,\n    31→} from \"./types.js\";\n    32→\n    33→let threadCounter = 0;\n    34→\n    35→function createThreadId(): string {\n    36→  return `thread_${Date.now()}_${++threadCounter}`;\n    37→}\n    38→\n    39→export function addUsage(a: TokenUsage, b: TokenUsage): TokenUsage {\n    40→  return {\n    41→    inputTokens: a.inputTokens + b.inputTokens,\n    42→    outputTokens: a.outputTokens + b.outputTokens,\n    43→    totalTokens: a.totalTokens + b.totalTokens,\n    44→    cacheReadTokens:\n    45→      (a.cacheReadTokens ?? 0) + (b.cacheReadTokens ?? 0) || undefined,\n    46→    cacheWriteTokens:\n    47→      (a.cacheWriteTokens ?? 0) + (b.cacheWriteTokens ?? 0) || undefined,\n    48→  };\n    49→}\n    50→\n    51→function extractUsage(response: AIMessage): TokenUsage {\n    52→  const meta = (response as any).usage_metadata;\n    53→  return {\n    54→    inputTokens: meta?.input_tokens ?? 0,\n    55→    outputTokens: meta?.output_tokens ?? 0,\n    56→    totalTokens: meta?.total_tokens ?? 0,\n    57→  };\n    58→}\n    59→\n    60→function extractTextContent(content: string | unknown): string {\n    61→  if (typeof content === \"string\") return content;\n    62→  if (Array.isArray(content)) {\n    63→    return content\n    64→      .map((block: any) => {\n    65→        if (typeof block === \"string\") return block;\n    66→        if (block.type === \"text\") return block.text;\n    67→        return JSON.stringify(block);\n    68→      })\n    69→      .join(\"\");\n    70→  }\n    71→  return JSON.stringify(content);\n    72→}\n    73→\n    74→function compactError(error: unknown, maxLen = 500): string {\n    75→  const msg =\n    76→    error instanceof Error\n    77→      ? `${error.name}: ${error.message}`\n    78→      : String(error);\n    79→  return msg.length > maxLen ? msg.substring(0, maxLen) + \"...\" : msg;\n    80→}\n    81→\n    82→function createThread(): AgentThread {\n    83→  const now = Date.now();\n    84→  return {\n    85→    id: createThreadId(),\n    86→    events: [],\n    87→    metadata: {},\n    88→    createdAt: now,\n    89→    updatedAt: now,\n    90→  };\n    91→}\n    92→\n    93→export class DeepFactorAgent<\n    94→  TTools extends StructuredToolInterface[] = StructuredToolInterface[],\n    95→> {\n    96→  private modelOrString: BaseChatModel | string;\n    97→  private resolvedModel: BaseChatModel | undefined;\n    98→  private tools: TTools;\n    99→  private instructions: string;\n   100→  private stopConditions: StopCondition[];\n   101→  private verifyCompletion: DeepFactorAgentSettings[\"verifyCompletion\"];\n   102→  private composedMiddleware: ComposedMiddleware;\n   103→  private interruptOn: string[];\n   104→  private contextManager: ContextManager;\n   105→  private onIterationStart?: (iteration: number) => void;\n   106→  private onIterationEnd?: (iteration: number, result: unknown) => void;\n   107→  private modelId: string;\n   108→\n   109→  constructor(settings: DeepFactorAgentSettings<TTools>) {\n   110→    this.modelOrString = settings.model;\n   111→\n   112→    if (typeof settings.model === \"string\") {\n   113→      this.modelId = settings.model;\n   114→    } else {\n   115→      this.modelId =\n   116→        (settings.model as any).modelName ??\n   117→        (settings.model as any).model ??\n   118→        (settings.model as any).name ??\n   119→        \"unknown\";\n   120→    }\n   121→\n   122→    this.tools = (settings.tools ?? []) as TTools;\n   123→    this.instructions = settings.instructions ?? \"\";\n   124→    this.verifyCompletion = settings.verifyCompletion;\n   125→    this.interruptOn = settings.interruptOn ?? [];\n   126→    this.onIterationStart = settings.onIterationStart;\n   127→    this.onIterationEnd = settings.onIterationEnd;\n   128→\n   129→    // Normalize stopWhen\n   130→    if (!settings.stopWhen) {\n   131→      this.stopConditions = [];\n   132→    } else if (Array.isArray(settings.stopWhen)) {\n   133→      this.stopConditions = settings.stopWhen;\n   134→    } else {\n   135→      this.stopConditions = [settings.stopWhen];\n   136→    }\n   137→\n   138→    // Compose middleware\n   139→    const middlewareList: AgentMiddleware[] = settings.middleware ?? [];\n   140→    this.composedMiddleware = composeMiddleware(middlewareList);\n   141→\n   142→    // Context management\n   143→    this.contextManager = new ContextManager(settings.contextManagement);\n   144→  }\n   145→\n   146→  private async ensureModel(): Promise<BaseChatModel> {\n   147→    if (this.resolvedModel) return this.resolvedModel;\n   148→    if (typeof this.modelOrString === \"string\") {\n   149→      this.resolvedModel = await initChatModel(this.modelOrString);\n   150→      return this.resolvedModel;\n   151→    }\n   152→    this.resolvedModel = this.modelOrString;\n   153→    return this.resolvedModel;\n   154→  }\n   155→\n   156→  private buildMessages(thread: AgentThread): BaseMessage[] {\n   157→    const messages: BaseMessage[] = [];\n   158→\n   159→    // Build context injection from summaries\n   160→    const contextInjection =\n   161→      this.contextManager.buildContextInjection(thread);\n   162→    if (this.instructions || contextInjection) {\n   163→      const system = [contextInjection, this.instructions]\n   164→        .filter(Boolean)\n   165→        .join(\"\\n\\n\");\n   166→      messages.push(new SystemMessage(system));\n   167→    }\n   168→\n   169→    // Convert thread events to messages\n   170→    for (const event of thread.events) {\n   171→      switch (event.type) {\n   172→        case \"message\": {\n   173→          if (event.role === \"user\") {\n   174→            messages.push(new HumanMessage(event.content));\n   175→          } else if (event.role === \"assistant\") {\n   176→            messages.push(new AIMessage(event.content));\n   177→          } else if (event.role === \"system\") {\n   178→            messages.push(new HumanMessage(`[System]: ${event.content}`));\n   179→          }\n   180→          break;\n   181→        }\n   182→        case \"human_input_received\": {\n   183→          messages.push(\n   184→            new HumanMessage(`[Human Response]: ${event.response}`),\n   185→          );\n   186→          break;\n   187→        }\n   188→        case \"summary\": {\n   189→          // Summaries are injected via context injection in the system prompt\n   190→          break;\n   191→        }\n   192→        default:\n   193→          break;\n   194→      }\n   195→    }\n   196→\n   197→    return messages;\n   198→  }\n   199→\n   200→  private isPendingHumanInput(thread: AgentThread): boolean {\n   201→    const requestEvents = thread.events.filter(\n   202→      (e) => e.type === \"human_input_requested\",\n   203→    );\n   204→    const responseEvents = thread.events.filter(\n   205→      (e) => e.type === \"human_input_received\",\n   206→    );\n   207→    return requestEvents.length > responseEvents.length;\n   208→  }\n   209→\n   210→  private checkInterruptOn(\n   211→    thread: AgentThread,\n   212→    iteration: number,\n   213→  ): string | null {\n   214→    if (this.interruptOn.length === 0) return null;\n   215→\n   216→    for (let i = thread.events.length - 1; i >= 0; i--) {\n   217→      const event = thread.events[i];\n   218→      if (event.iteration !== iteration) break;\n   219→      if (\n   220→        event.type === \"tool_call\" &&\n   221→        this.interruptOn.includes(event.toolName)\n   222→      ) {\n   223→        return event.toolName;\n   224→      }\n   225→    }\n   226→    return null;\n   227→  }\n   228→\n   229→  async loop(prompt: string): Promise<AgentResult | PendingResult> {\n   230→    const thread = createThread();\n   231→    return this.runLoop(thread, prompt, 1);\n   232→  }\n   233→\n   234→  private async runLoop(\n   235→    thread: AgentThread,\n   236→    prompt: string,\n   237→    startIteration: number,\n   238→  ): Promise<AgentResult | PendingResult> {\n   239→    const model = await this.ensureModel();\n   240→\n   241→    // Push initial user message (only if this is the first call, not a resume)\n   242→    if (startIteration === 1) {\n   243→      thread.events.push({\n   244→        type: \"message\",\n   245→        role: \"user\",\n   246→        content: prompt,\n   247→        timestamp: Date.now(),\n   248→        iteration: 0,\n   249→      });\n   250→    }\n   251→\n   252→    let totalUsage: TokenUsage = {\n   253→      inputTokens: 0,\n   254→      outputTokens: 0,\n   255→      totalTokens: 0,\n   256→    };\n   257→    let iteration = startIteration;\n   258→    let consecutiveErrors = 0;\n   259→    let lastResponse = \"\";\n   260→\n   261→    // Merge middleware tools with user tools\n   262→    const allTools: StructuredToolInterface[] = [\n   263→      ...this.tools,\n   264→      ...this.composedMiddleware.tools,\n   265→    ];\n   266→\n   267→    while (true) {\n   268→      // Callback\n   269→      this.onIterationStart?.(iteration);\n   270→\n   271→      // Middleware: beforeIteration\n   272→      const middlewareCtx: MiddlewareContext = {\n   273→        thread,\n   274→        iteration,\n   275→        settings: {\n   276→          model: this.modelOrString,\n   277→          tools: this.tools,\n   278→          instructions: this.instructions,\n   279→        } as DeepFactorAgentSettings,\n   280→      };\n   281→      await this.composedMiddleware.beforeIteration(middlewareCtx);\n   282→\n   283→      // Context management: check if summarization needed\n   284→      if (this.contextManager.needsSummarization(thread)) {\n   285→        await this.contextManager.summarize(thread, model);\n   286→      }\n   287→\n   288→      // Build messages from thread\n   289→      const messages = this.buildMessages(thread);\n   290→\n   291→      try {\n   292→        // Bind tools and run inner tool-calling loop\n   293→        const modelWithTools =\n   294→          allTools.length > 0 && model.bindTools\n   295→            ? model.bindTools(allTools)\n   296→            : model;\n   297→\n   298→        let stepCount = 0;\n   299→        let iterationUsage: TokenUsage = {\n   300→          inputTokens: 0,\n   301→          outputTokens: 0,\n   302→          totalTokens: 0,\n   303→        };\n   304→        let humanInputRequested = false;\n   305→        let lastAIResponse: AIMessage | null = null;\n   306→\n   307→        while (stepCount < 20) {\n   308→          const response = (await modelWithTools.invoke(\n   309→            messages,\n   310→          )) as AIMessage;\n   311→          messages.push(response);