{
  "name": "deep-factor-tui",
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "deep-factor-tui": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
